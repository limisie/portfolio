---
import About from "@components/organisms/About.astro";
import Hero from "@components/organisms/Hero.astro";
import CardList from "@components/organisms/CardList.astro";
import Layout from "@layouts/Layout.astro";

import { getAllProjects } from "@scripts/data.js";

const projects = await getAllProjects();
---

<Layout>
	<Hero />
	<section>
		<About />
	</section>
	<section>
		<CardList listHeader="My work" href="/portfolio" list={projects} />
	</section>
</Layout>
